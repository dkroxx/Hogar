
@{
    ViewBag.Title = "IniciarSesion";
}

<script src="~/Scripts/Funciones/IniciarSesion.js"></script>

<div class="container">
    <div id="Formulario" class="col-lg-12" align="center">
        <div class="col-lg-12">
            <img src="~/Images/logo.jpg" alt="Alternate Text" style="padding:20px; width: 800px; height:400px;" />
        </div>
        <div class="col-lg-12" style="padding:20px;">
            <form>
                <div class="form-group">
                    <label for="Usuario">Usuario</label>
                    <input type="text" class="form-control" id="Usuario">
                </div>
                <div class="form-group">
                    <label for="Contrasena">Contraseña:</label>
                    <input type="password" class="form-control" id="Contrasena">
                </div>
            </form>
            <button type="submit" class="btn btn-default">Submit</button>
        </div>                
    </div>
</div>

